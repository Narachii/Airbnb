.head-content.hosts{style: "padding:0;"}
	.icon{style: "width:100px;"}
		=link_to image_tag("logo2.jpg", class: "logo"),root_path,method: :get
	.search-bar
		%form.search{action: "users/search"}
			.search-bar__container
				=form_tag "/users/index", method: :get do
					%input.search{"aria-label" => "Search", :autocomplete => "off", :name => "location", :placeholder => "Search", :type => "text"}
	%ul.lists
		-if user_signed_in?
			%li.info_list
				=link_to "Log Out",destroy_user_session_path, method: :delete,class: "info"
			%li.info_list
				=link_to "Help",root_path,class: "info"
			%li.info_list
				=link_to "Messages",root_path,class: "info"
			-if current_user.host && @room == nil
				%li.info_list
					=link_to "Host","/users/#{current_user.id}/hosts/#{current_user.host.id}",class: "info"
			-elsif current_user.host
				%li.info_list
					=link_to "Host","/users/#{current_user.id}/hosts/#{current_user.host.id}/rooms/#{@room.id}/room_steps/home",class: "info"
		-else
			%li.info_list
				%div{class: "info",id: "Login"}Log In
			%li.info_list
				%div{class: "info",id: "signUp"} Sign Up
			%li.info_list
				=link_to "Help",root_path,class: "info"
